<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - Social</title>
	<link href="~/images/favicon.png" rel="icon" type="image/x-icon" />
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/css/tailwind.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/SocialApp.styles.css" asp-append-version="true" />
</head>
<body>
	<div id="wrapper" class="">
		@* Topbar *@
		<header class="z-\[10000\] h-[--m-top] fixed top-0 lsft-0 w-full flex items-center bg-white\/80 sky-50
		backdrop-blur-xl border-b border-slate-200">
		<div>
			<div>			</div>
			<div>
				@* search *@
				@* notification icon *@
				@* profile icon *@
			</div>
		
		</div>
		</header>
		@* Side bar (left) *@

		@RenderBody()
	</div>

	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>

	@* Asset scripts *@
	<script src="~/js/uikit.min.js"></script>
	<script src="~/js/simplebar.js"></script>
	<script src="~/js/script.js"></script>

	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
